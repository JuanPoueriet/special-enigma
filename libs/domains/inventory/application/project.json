{
  "name": "inventory-application",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/domains/inventory/application/src",
  "projectType": "library",
  "tags": [
    "scope:inventory",
    "type:application",
    "platform:agnostic",
    "criticality:high"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/domains/inventory/application",
        "main": "libs/domains/inventory/application/src/index.ts",
        "tsConfig": "libs/domains/inventory/application/tsconfig.lib.json",
        "assets": ["libs/domains/inventory/application/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/domains/inventory/application"],
      "options": {
        "jestConfig": "libs/domains/inventory/application/jest.config.cts",
        "passWithNoTests": true
      }
    }
  }
}
