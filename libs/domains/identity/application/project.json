{
  "name": "identity-application",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/domains/identity/application/src",
  "projectType": "library",
  "tags": [
    "scope:identity",
    "type:application",
    "platform:agnostic",
    "tenant-mode:schema",
    "criticality:high"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/domains/identity/application",
        "main": "libs/domains/identity/application/src/index.ts",
        "tsConfig": "libs/domains/identity/application/tsconfig.lib.json",
        "assets": [
          "libs/domains/identity/application/*.md"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/domains/identity/application/jest.config.cts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  },
  "prefix": "virteex"
}