{
  "name": "purchasing-infrastructure",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/domains/purchasing/infrastructure/src",
  "projectType": "library",
  "tags": [
    "scope:purchasing",
    "type:infrastructure",
    "platform:agnostic",
    "criticality:high"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "rootDir": ".",
        "outputPath": "dist/libs/domains/purchasing/infrastructure",
        "main": "libs/domains/purchasing/infrastructure/src/index.ts",
        "tsConfig": "libs/domains/purchasing/infrastructure/tsconfig.lib.json",
        "assets": [
          "libs/domains/purchasing/infrastructure/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/domains/purchasing/infrastructure/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/domains/purchasing/infrastructure"
      ],
      "options": {
        "jestConfig": "libs/domains/purchasing/infrastructure/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}